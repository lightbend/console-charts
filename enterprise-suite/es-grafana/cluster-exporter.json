[
   {
      "graphName": "CPU Percent",
      "promQL": [
         {
            "expr": "sum by (instance) (rate(container_cpu_user_seconds_total[1m])) + sum by (instance) (rate(container_cpu_system_seconds_total[1m]))",
            "legendFormat": "container_cpu {{ instance }}"
         }
      ],
      "yaxes": {
         "format": "percentunit",
         "min": 0
      }
   },
   {
      "graphName": "Memory Available Percent",
      "promQL": [
         "node_memory_usable_percent"
      ]
   },
   {
      "graphName": "File System Free Percent",
      "promQL": [
         "node_filesystem_free_percent"
      ]
   },
   {
      "graphName": "Bytes Received Per Second",
      "promQL": [
         "sum by (instance) (irate(node_network_receive_bytes[1m]))"
      ]
   },
   {
      "graphName": "Bytes Transmitted Per Second",
      "promQL": [
         "sum by (instance) (irate(node_network_transmit_bytes[1m]))"
      ]
   }
]
