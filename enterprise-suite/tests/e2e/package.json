    "style-loader": "0.13.2",
    "stylus-loader": "3.0.1",
    "ts-loader": "4.4.2",
    "ts-node": "3.2.2",
    "ts-node-dev": "1.0.0-pre.26",
    "tslint": "5.11.0",
    "tslint-jasmine-rules": "1.5.1",
    "typescript": "3.0",
