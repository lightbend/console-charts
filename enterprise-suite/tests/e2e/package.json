{
  "name": "es-console-spa-e2e",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "9.4.0"
  },
  "scripts": {
    "e2e": "cypress run",
    "e2e:minikube": "cypress run --env configFile=minikube",
    "e2e:travis-prs": "NO_COLOR=1 cypress-run --env configFile=minikube",
    "e2e:travis-prs-subset1": "NO_COLOR=1 cypress-run --spec 'cypress/integration/+(cluster-page|grafana|monitor-history|workload-page).spec.ts' --env configFile=minikube",
    "e2e:travis-prs-subset2": "NO_COLOR=1 cypress-run --spec 'cypress/integration/+(create-monitor|local-prom-health).spec.ts' --env configFile=minikube",
    "e2e:travis-prs:result": "NO_COLOR=1 cypress-run --env configFile=minikube,skipKnownError=true | tee results.txt",
    "e2e:check-results": "./scripts/check_e2e_results.sh",
    "e2e:patch-minikube-ip": "./scripts/patch-minikube-ip.sh",
    "e2e:wait-es-services": "./scripts/wait_es_services.sh",
    "e2e:travis": "cypress run --env configFile=minikube,skipKnownError=true",
    "e2e:demo-app-setup": "kubectl apply -f ./config/es-demo.yaml",
    "cypress:open": "cypress open",
    "cypress:minikube:open": "cypress open --env configFile=minikube",
    "cypress:minikube:skip-error:open": "cypress open --env configFile=minikube,skipKnownError=true"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "1.6.7",
    "@angular/compiler-cli": "5.2.0",
    "@angular/language-service": "5.2.0",
    "@cypress/webpack-preprocessor": "2.0.1",
    "@types/cucumber": "4.0.4",
    "@types/lodash": "4.14.104",
    "autoprefixer": "7.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "codelyzer": "4.0.2",
    "command-exists": "1.2.7",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.8",
    "cssnano": "3.10.0",
    "cucumber": "4.2.1",
    "cypress": "3.1.5",
    "cypress-cucumber-preprocessor": "1.2.2",
    "cypress-run": "1.0.7",
    "exports-loader": "0.6.4",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jsverify": "0.8.3",
    "less-loader": "4.0.5",
    "npm-run-all": "4.1.3",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.10",
    "postcss-url": "7.3.0",
    "raw-loader": "0.5.1",
